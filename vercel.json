{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "static/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/results",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/results",
            "methods": ["POST", "GET"],
            "dest": "/api/index.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/static/$1"
        }
    ]
}
